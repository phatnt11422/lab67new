<section class="login-clean">
    <form method="post" action="/">
        <h2 class="sr-only">Login Form</h2>
        <div class="illustration">
            <img width="200" height="100" src="assets/fonts/tinder.png">
        </div>
        <div class="form-group">
            <input class="form-control" type="text" name="hoten" placeholder="Họ tên">
        </div>
        <div class="form-group">
            <input class="form-control" type="email" name="email" placeholder="Email">
        </div>
        <div class="form-group">
            <input class="form-control" type="password" name="password" placeholder="Password">
        </div>
        <div class="form-group">
            <button class="btn btn-primary btn-block" type="submit" onclick="notify()" >Sign Up</button>
        </div>
        <a class="forgot" href="/">Back</a>

    </form>
</section>
<script src="assets/js/jquery.min.js"></script>
<script src="assets/bootstrap/js/bootstrap.min.js"></script>
<script>
    function notify() {
        alert('Đăng ký thành công');
    }
</script>